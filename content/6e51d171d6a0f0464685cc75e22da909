<script type="text/javascript" src="/assets/js/jquery-1.3.2.min.js"></script>
<script type="text/javascript" src="/assets/js/jquery-ui-1.7.1.custom.min.js"></script>

<script type="text/javascript">
var SliderValue = 9;
    
$(document).ready(function()
    {
GetThumbnail();
        $("#slider").slider({ 
        steps: 1, min: 1, max: 18, value:9,
        stop: function(e,ui) 
            {
            SliderValue = ui.value;
            GetThumbnail();
            }
         });
    });
    
function GetThumbnail()
    {
    if (checkUrl())
        {
        var _width = -1;
        var _height = -1;
        switch(parseInt($("#ddlBrowserSize").val()))
        {
        case 0: _width=-1; break;
        case 1: _width=800; _height=600; break;
        case 2: _width=1024; _height=768; break;
        }
        $("#grab").load('/_____getimage.ashx?url=' + $("#ctl00_cphMain_txtUrl").val() + '&size=' + SliderValue + '&width=' + _width + '&height=' + _height, 
            function()
                {
                $("#latest").fadeIn("slow");
                });
        }
    }
    
function checkUrl()
    {
    var regex = /https?:\/\/([-\w\.]+)+(:\d+)?(\/([\w/_\.]*(\?\S+)?)?)?/;
    if (regex.test($("#ctl00_cphMain_txtUrl").val()))
        {
        $("#msg").html('')
        return true;
        }
        else
        {
        $("#msg").html('Please enter a valid URL');
        $("#latest").fadeOut("slow");
        return false;
        }
    }
    </script>



