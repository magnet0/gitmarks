<script type="text/javascript">

    $(document).ready(function () {

        $("div.error404page input[id='searchfor']").keypress(function (e) {
            var code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13 || code == 10) {
                DoSearchFrom404Error();
                return false;
            }
            return true;
        });

        $("div.btnupdate img").click(function () {
            DoSearchFrom404Error();
            return false;
        });

    });

    function DoSearchFrom404Error() {
        var value = $("div.error404page input[id='searchfor']").val();
        SearchByWordandDefault(value);
    }


</script>
