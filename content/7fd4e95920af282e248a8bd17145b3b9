<div id="flipAd" name="flipAd"></div>

<script type="text/javascript">
var xmlPath;

 if(typeof showId == "undefined"){
   xmlPath = "/assets/xml/flippy/flippy.xml";
  } else { 
   xmlPath = "/assets/xml/flippy/"+ showId +"/flippy.xml";
 }
 

var loadFlippy = function(xmlPath){
	var flipSO = new SWFObject("/assets/swf/flippy.swf", "flipAd", "300", "80", "9", "#09212B");
	flipSO.addParam("quality", "high");
	flipSO.addParam("allowScriptAccess", "sameDomain");
	flipSO.addParam("play", "true");
	flipSO.addParam("autoPlay", "true");
	flipSO.addParam("salign", "top");
	flipSO.addParam("wmode", "transparent");
	flipSO.addVariable("path", xmlPath);
	flipSO.write("flipAd");
}

loadFlippy(xmlPath);
</script>
