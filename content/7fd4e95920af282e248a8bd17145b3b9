jqN('#mainform').submit(function() {
	if(checkForm(this)){
		jqN.ajax({
			type: "POST",
			url: "/app/mobile/sendToPhone.php",
			data: "show_id="+showId+"&area_code="+jqN('#area_code').val()+"&prefix="+jqN('#prefix').val()+"&line_number="+jqN('#line_number').val()+"&provider_code="+jqN('#provider_code').val()+"&from="+jqN('#from').val()+"",
			error: function(XMLHttpRequest, textStatus, errorThrown) {
			alert(XMLHttpRequest+' '+textStatus+' '+errorThrown)
			},
			success: function(msg){
				jqN('#mainform').empty();
				jqN('#mainform').append('<p style="font-family:Verdana; font-size:9px; font-weight:normal">Thanks for Signing Up!</p>');
			}
		});
	}
